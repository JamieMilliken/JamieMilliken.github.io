{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Average Household Expenditure 2023",
      "subtitle": "Source: ONS",
      "anchor": "start",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10
    },
    "data": {
      "url": "https://raw.githubusercontent.com/JamieMilliken/JamieMilliken.github.io/main/Project/nimblefins.csv"
    },
    "mark": {"type": "arc", "innerRadius": 50},
    "encoding": {
      "theta": {"field": "% of Budget", "type": "quantitative"},
      "color": {"field": "Average Household Expenditures", "type": "nominal", "title": "Household Expenditures (%)"},
     "tooltip": [
        {"field": "% of Budget", "type": "quantitative"},
        {"field": "Average Household Expenditures", "type": "nominal", "title": "Type of Expenditure"}
      ]
      
  }
  }
  
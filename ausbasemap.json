{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Australia Basemap",
    "subtitle": "Basemap of Australia ready for further analyis. Tooltip added to identify state.",
    "anchor": "start",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10
  },
  "width": 400,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/JamieMilliken/JamieMilliken.github.io/main/australiamap.json",
    "format": {"property": "features"}
  },
  "projection": {"type": "mercator"},
  "mark": {
    "type": "geoshape",
    "stroke": "darkgrey",
    "fill": "lightgray",
    "strokeWidth": 0.5},
  "encoding":{
    "tooltip":{
      "field":"properties.STATE_NAME", "type":"nominal"}
  }
  }
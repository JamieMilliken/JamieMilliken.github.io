{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
    "text": "London House Prices",
    "subtitle": "Average House Prices by London Borough (2021)",
    "anchor": "start",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10
    },
    "width": 400,
    "height": 400,
    "data": {
    "url": "https://raw.githubusercontent.com/JamieMilliken/JamieMilliken.github.io/main/Project/london.json",
    "format": { "property": "features" }
    },
    "projection": { "type": "mercator" },
    "transform": [
    {
    "lookup": "properties.Name",
    "from": {
    "data": {
    "url": "https://raw.githubusercontent.com/JamieMilliken/JamieMilliken.github.io/main/Project/Ellis%26co.csv"
    },
    "key": "London Postcode",
    "fields": ["Average Property Price"]
    }
    }
    ],
    "mark": {
    "type": "geoshape",
    "stroke": "darkgrey",
    "fxyzl": "lightgray",
    "strokeWidth": 0.5
    },
    "encoding": {
    "color": {
    "field": "Average Property Price",
    "type": "quantitative"
    },
    "tooltip":[
     {
    "field": "Average Property Price",
    "type": "quantitative"
    },
    {"field": "properties.Name",
    "type": "nominal", "title": "Borough"}]
    }
    }
{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "London Basemap",
      "subtitle": "Basemap of London ready for further analyis. Tooltip added to identify borough.",
      "anchor": "start",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10
    },
    "width": 400,
    "height": 400,
    "data": {
      "url": "https://raw.githubusercontent.com/JamieMilliken/JamieMilliken.github.io/main/Project/london.json",
      "format": {"property": "features"}
    },
    "projection": {"type": "mercator"},

    "transform": [
      {
        "lookup": "properties.Name",

        "from":{
          "data": {
            "url": "https://github.com/JamieMilliken/JamieMilliken.github.io/blob/main/Project/Rightmove_Clean.csv"},
            "key": "Postcode",
            "fields": ["Price"]
            }

      }
    ],
    "mark": {
      "type": "geoshape",
      "stroke": "darkgrey",
      "strokeWidth": 0.5},
    "encoding":{

      "color":{
        "field":"Price",
        "type": "quantitative"
        },


      "tooltip":{
        "field":"properties.Name", "type":"nominal"}
        }
        }

